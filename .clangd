CompileFlags:
  Add:
    - -std=c++23
    - -stdlib=libc++
    - -ferror-limit=0
    - -Iinclude
    - -pedantic
    - -Werror
    - -Wall
    - -Wextra
    - -Wcast-align
    - -Wconversion
    - -Wdouble-promotion
    - -Wnon-virtual-dtor
    - -Wold-style-cast
    - -Woverloaded-virtual
    - -Wshadow
    - -Wsign-conversion
    - -Wsign-promo
    - -Wunused
    - -Wformat=2
    - -Wodr
    - -Wattributes
    - -Wnull-dereference
Diagnostics:
  UnusedIncludes: None
  MissingIncludes: None
